   "outputs": [],
   "source": [
    "nomsFichier = ['ascii']\n",
    "import tarfile\n",
    "\n",
    "for file in nomsFichier:\n",
    "    fname = file +'/' + file + '.tgz'\n",
    "    tar = tarfile.open(fname, \"r:gz\")\n",
    "    tar.extractall(path = file+'/')\n",
    "    tar.close()"
   ]
  }
